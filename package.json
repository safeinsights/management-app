{
    "name": "management-app",
    "version": "0.0.1",
    "private": true,
    "description": "The SafeInsights management app serves as an interface for researchers to submit and members to approve study proposals.",
    "license": "AGPL-3.0-or-later",
    "author": "Nathan Stitt",
    "main": "index.js",
    "scripts": {
        "build:docker:production": "./bin/prod-docker-build",
        "build:docker:ci": "./bin/prod-docker-build ci-only-do-not-push",
        "dev": "next dev --turbopack -p 4000",
        "prepare": "panda codegen",
        "build": "next build",
        "start": "next start",
        "deploy": "tsx bin/deploy.ts",
        "ci": "run-p checks test:e2e test:unit",
        "lint": "next lint && prettier --check .",
        "lint:fix": "next lint --fix && prettier --write .",
        "pre:push": "run-s lint:fix typecheck",
        "typecheck": "cd src && tsc --noEmit",
        "update-db-types": "kysely-codegen --out-file src/database/types.ts",
        "checks": "run-p -c typecheck lint",
        "test:e2e": "playwright test",
        "test:unit": "vitest",
        "test": "run-p -c test:*",
        "pre:commit": "npx lint-staged"
    },
    "dependencies": {
        "@aws-sdk/client-ecr": "^3.665.0",
        "@aws-sdk/client-s3": "^3.670.0",
        "@aws-sdk/client-secrets-manager": "^3.670.0",
        "@aws-sdk/credential-providers": "^3.665.0",
        "@aws-sdk/lib-storage": "^3.687.0",
        "@aws-sdk/s3-request-presigner": "^3.687.0",
        "@clerk/nextjs": "^6.9.1",
        "@faker-js/faker": "^9.2.0",
        "@hookform/resolvers": "^3.9.0",
        "@mantine/core": "^7.15",
        "@mantine/form": "^7.15",
        "@mantine/modals": "^7.15",
        "@mantine/notifications": "^7.13.2",
        "@mantinex/dev-icons": "^1.0.2",
        "@sentry/nextjs": "^8.34.0",
        "@tabler/icons-react": "^3.19.0",
        "@tanstack/react-query": "^5.59.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/papaparse": "^5.3.15",
        "@types/uuid": "^10.0.0",
        "@vanilla-extract/css": "^1.16",
        "@vanilla-extract/next-plugin": "^2.4",
        "aws-iam-policy-types": "^1.0.1",
        "aws-sdk": "^2.1691.0",
        "child_process": "^1.0.2",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "debug": "^4.3.7",
        "highlight.js": "^11.10.0",
        "jsonwebtoken": "^9.0.2",
        "kysely": "^0.27.4",
        "kysely-ctl": "^0.9.0",
        "mantine-datatable": "^7.12.4",
        "mantine-form-zod-resolver": "^1.1.0",
        "next": "15.1.0",
        "next-swagger-doc": "^0.4",
        "papaparse": "^5.4.1",
        "pg": "^8.13.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-icons": "^5.3.0",
        "remeda": "^2.14.0",
        "server-only": "^0.0.1",
        "uuid": "^10.0.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.16.0",
        "@next/eslint-plugin-next": "^15.1.0",
        "@pandacss/dev": "^0.49.0",
        "@playwright/test": "^1.49",
        "@testing-library/react": "^16.1",
        "@types/react": "19.0.1",
        "@vanilla-extract/vite-plugin": "^4.0.18",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-v8": "^2.1.8",
        "dotenv": "*",
        "eslint": "^9.16.0",
        "eslint-config-next": "*",
        "eslint-config-prettier": "*",
        "eslint-plugin-react": "^7.37.2",
        "globals": "^15.13.0",
        "happy-dom": "*",
        "husky": "^9.1.6",
        "kysely-codegen": "^0.16.8",
        "lint-staged": "^15.2.10",
        "next-router-mock": "^0.9.13",
        "npm-run-all": "*",
        "pg-transactional-tests": "^1.2.0",
        "prettier": "*",
        "tsx": "^4.19.1",
        "typescript-eslint": "^8.18.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "*"
    },
    "overrides": {
        "react-remove-scroll": {
            "react": "^19.0.0"
        },
        "use-callback-ref": {
            "react": "^19.0.0"
        },
        "react-textarea-autosize": {
            "react": "^19.0.0"
        },
        "react-number-format": {
            "react": "^19.0.0",
            "react-dom": "^19.0.0"
        },
        "swr": {
            "react": "^19.0.0"
        }
    }
}
